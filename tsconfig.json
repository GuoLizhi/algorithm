{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "esnext",
      "es2015",
      "es2016",
      "es2017",
      "es2018",
      "es2019"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "strictNullChecks": false
  }
}
